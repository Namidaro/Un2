<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:evm="clr-namespace:Unicon2.Fragments.Programming.ViewModels.ElementViewModels">
    <DataTemplate x:Key="InputLibraryElement" DataType="{x:Type evm:OutputViewModel}">
        <Grid>
            <Path
                Data="M0.5,0.5 L19.5,0.5 20,0.70710692 24.292845,4.999952 20,9.2927972 19.5,9.5000001 0.5,9.5000001 z"
                RenderTransformOrigin="0.5,0.5"
                Stretch="Fill"
                Stroke="Black"
                StrokeLineJoin="Round"
                StrokeThickness="1">
                <Path.RenderTransform>
                    <TransformGroup>
                        <RotateTransform Angle="180" />
                    </TransformGroup>
                </Path.RenderTransform>
            </Path>
            <TextBlock
                Margin="6,0,0,0"
                HorizontalAlignment="Left"
                FontFamily="Calibri"
                FontSize="8"
                LineHeight="10"
                Text="{Binding Symbol}" />
        </Grid>
    </DataTemplate>
</ResourceDictionary>